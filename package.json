{
  "name": "rv-serve-directory",
  "version": "1.0.4",
  "description": "Serve a directory on the local network",
  "main": "index.js",
  "prettier": "@vighnesh153/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "local-server",
    "directory-serve",
    "serve-directory",
    "serve-directory-153",
    "serve",
    "directory",
    "file-transfer"
  ],
  "scripts": {
